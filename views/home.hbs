<div class="container">
  <div class="page-header">
    <h1 id="timeline">Intelligence</h1>
  </div>
  <ul id='intel_list' class="timeline">
  {{#each intels}}
    <li>
      <div class="timeline-badge"><i class="glyphicon glyphicon-check"></i></div>
      <div class="timeline-panel">
        <div class="timeline-heading">
          <h4 class="timeline-title">{{this.headline}}</h4>

          <p>
            <small class="text-muted"><i class="glyphicon glyphicon-time"></i> {{this.time}}</small>
          </p>
        </div>
        <div class="timeline-body">
          <p>{{this.content}}</p>
        </div>
      </div>
    </li>
  {{/each}}
  </ul>
</div>
